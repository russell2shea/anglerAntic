package {	import flash.display.MovieClip;	public class Eel extends MovieClip 	{		public var xSpeed:Number;	//pixels moved to the right per tick		public var ySpeed:Number;	//pixels moved downwards per tick				public function Eel( startX:Number, startY:Number ) 		{			x = startX;			y = startY;						xSpeed = 2;			ySpeed = 0;		}				public function moveABit():void 		{			x = x + xSpeed;			y = y + ySpeed;		}	}}