package {	import flash.display.MovieClip;	public class Jelly extends MovieClip 	{		public var xSpeed:Number;	//pixels moved to the right per tick		public var ySpeed:Number;	//pixels moved downwards per tick				public function Jelly( startX:Number, startY:Number ) 		{			x = startX;			y = startY;						xSpeed = randomRange(1,4);			ySpeed = randomRange(-1,1);						this.gotoAndStop(Math.floor(Math.random()*5+1));		}				public function randomRange(max:Number, min:Number = 0):Number		{    		return Math.random() * (max - min) + min;			}				public function moveABit():void 		{			x = x + xSpeed;			y = y + ySpeed;		}	}}