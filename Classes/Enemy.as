package {	import flash.display.MovieClip;		public class Enemy extends MovieClip 	{				public var xSpeed:Number;	//pixels moved to the right per tick		public var ySpeed:Number;	//pixels moved downwards per tick		//public var randomRange:Math.random() * (max - min) + min;				public function Enemy( startX:Number, startY:Number ) 		{			x = startX;			y = startY;						xSpeed = randomRange(1,-1);			ySpeed = randomRange(-1,-7);						this.gotoAndStop(Math.floor(Math.random()*5+1));		}				public function randomRange(max:Number, min:Number = 0):Number		{    		return Math.random() * (max - min) + min;			}								public function moveABit():void 		{						x = x + xSpeed;			y = y + ySpeed;		}					}}